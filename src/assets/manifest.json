{
  "id": "starfish.labs.elanodes",
  "version": "0.8.3",
  "version_code": 14,
  "name": "ELAnodes",
  "short_name": "ELAnodes",
  "short_description": "Staking & rewards utilities",
  "description": "Elastos staking utilities and personal rewards dashboard",
  "long_description": "Your go to resource for Elastos staking. Inside you will find supernode details, easy-to-use voting functionality, a personal rewards dashboard, and general charts and analytics for the Elastos ecosystem.",
  "start_url": "index.html",
  "type": "app",
  "start_visible": "hide",
  "category": "finance",
  "icons": [
    {
      "src": "assets/images/logo.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "assets/images/logo.png",
      "sizes": "128x128",
      "type": "image/png"
    }
  ],
  "author": {
    "name": "Starfish Labs",
    "email": "hello@starfishlabs.dev",
    "website": "https://starfishlabs.dev"
  },
  "default_locale": "en",
  "plugins": ["InAppBrowser", "Clipboard", "Device", "NetworkStatus"],
  "urls": ["https://elanodes.com/api/*", "https://node1.elaphant.app/*"],
  "startup_service": ["stakingStatusAlerts"],
  "intent_filters": [
    {
      "action": "elanodes-stakingtools"
    },
    {
      "action": "elanodes-addwallet"
    },
    {
      "action": "elanodes-viewreport"
    }
  ],
  "platform": ["android", "ios"],
  "background_color": "#4e8ef7",
  "theme_display": "show",
  "theme_color": "#4e8ef7"
}
